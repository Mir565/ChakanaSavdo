<script>
  var data=<%- JSON.stringify(data)%>;
  console.log(data)
  document.getElementById('cusname').innerHTML+=`${data[0].old.customername}`
  document.getElementById('vaqt').innerHTML+=new Date().toLocaleString();
  document.getElementById('taked_tmie').innerHTML+=data[0].old.cr_date;
function createTable(data){
  var allprice=0;
 ss=`<tr>
     <th style="width:0.5%">Nomer</th>
    <th style="width:40%">Nomi</th>
    <th>Soni</th>
    <th>1 Dona</th>
    <th>Jami</th>
  </tr>`;
 for (let i=0;i<data.length;i++)
 {
  ss+=`
  <tr>
    <td>${i+1}</td>
    <td>${data[i].old.name}</td>
    <td>${data[i].vazvratcount}</td>
    <td>${data[i].vazvratprice}</td>
    <td>${data[i].vazvratcount*data[i].vazvratprice}</td>
  </tr>
 `
 allprice=allprice+(data[i].vazvratcount*data[i].vazvratprice);
 }
 
 
          document.getElementById('all').innerHTML = `Jami ${allprice}`
 document.getElementById('tablehref').innerHTML=ss;
}
createTable(data)
window.print();
window.onafterprint=()=>{
  window.close();
}
</script>
